<div class="container">
  <div class="row">
    <div class="col-md-6">
      <h1>completo</h1>
      <% @todos.each do |v| %>
        <% if v.completed == true %>
        <p> <%= v.description %> </p>
        <% end %>
      <% end %>
    </div>
    <div class="col-md-6">
      <h1>incompleto</h1>
      <% @todos.each do |v| %>
        <% if v.completed != true %>
        <p> <%= v.description %> </p>
        <% end %>
      <% end %>
    </div>
  </div>
</div>
